<form method="post">
    <table>
        <tr>
            <th>
                REGISTRATION
            </th>
        </tr>
        <tr>
            <td>NAME</td>
            <td><input type="text" name="name" placeholder="ENTER THE NAME"/> </td>
        </tr>
        <tr>
            <td>EMAIL</td>
            <td><input type="email" name="email" placeholder="ENTER THE EMAIL"/> </td>
        </tr> 
        <tr>
            <td>PASSWORD</td>
            <td><input type="password" name="password" placeholder="ENTER THE PASSWORD"/> </td>
        </tr>
        <tr>
            <td>CITY</td>
            <td><input type="text" name="city" placeholder="ENTER THE CITY"/> </td>
        </tr>
        <tr>
            <td>HOBBY</td>
            <td><input type="text" name="hobby" placeholder="ENTER THE HOBBY"/> </td>
        </tr>
        <tr>
            <td>CONTACT NO </td>
            <td><input type="int" name="contact_no" placeholder="ENTER THE CONTACT NO "/> </td>
        </tr>
        <tr>
            <td>ADDRESS</td>
            <td><textarea name="address" placeholder="ENTER THE ADDRESS" ></textarea> </td>
        </tr>
        <tr>
            <td>DATE OF BIRTH</td>
            <td><input type="date" name="dob" placeholder="ENTER THE DATE OF BIRTH"/> </td>
        </tr>
        <tr>
            <td></td>
            <td><input type="submit" name="submit"/> </td>
          
        </tr>
    </table>
</form>

